{
    "version": "1.0.0",
    "name": "boilerplate-preact",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run dev",
        "dev": "npm run parcel:watch",
        "build": "npm run parcel:build",
        "parcel:watch": "parcel ./src/index.html",
        "parcel:build": "parcel build ./src/index.html --no-source-maps"
    },
    "alias": {
        "react": "preact/compat",
        "react-dom": "preact/compat"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.12.10",
        "@babel/preset-typescript": "^7.12.7",
        "autoprefixer": "^10.2.1",
        "parcel": "^2.0.0-beta.1",
        "tailwindcss": "^2.0.2",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "preact": "^10.5.9"
    }
}
